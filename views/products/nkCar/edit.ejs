                  </div>
                  <div class="form-group col-md-6">
                    <label for="macs_CRFTime">CRF Start Time</label>
                    <input type="text" class="timeInput form-control col-md-6" name="Macs[macs_CRFTime]"
                      value="<%= data.macs_CRFTime %>" placeholder="HH:mm (24-hr)"
                      pattern="([0-1]{1}[0-9]{1}|20|21|22|23):[0-5]{1}[0-9]{1}" maxlength="5">
                  </div>
                  <div class="form-group col-md-6">
                    <label for="macs_CRFEndTime">CRF End Time</label>
                    <input type="text" class="timeInput form-control col-md-6" name="Macs[macs_CRFEndTime]"
                      value="<%= data.macs_CRFEndTime %>" placeholder="HH:mm (24-hr)"
                      pattern="([0-1]{1}[0-9]{1}|20|21|22|23):[0-5]{1}[0-9]{1}" maxlength="5">
                  </div>
                  <div class="form-group col-md-6">
                    <label for="macs_MVETime">MVE Storage Time</label>
                    <input type="text" class="timeInput form-control col-md-6" name="Macs[macs_MVETime]"
                      value="<%= data.macs_MVETime %>" placeholder="HH:mm (24-hr)"
                      pattern="([0-1]{1}[0-9]{1}|20|21|22|23):[0-5]{1}[0-9]{1}" maxlength="5">
                  </div>
                </div>
                <% if(data.macs_lock === null){ %>
                <div class="form-row">
                  <div class="submitBtn col-md-6 col-6">
                    <button type="submit" class="btn btn-warning">Save</button>
                  </div>
                  <div class="lockBtn col-md-6 col-6">
                    <button class="btn btn-danger" name="Macs[lock]" type="submit" value="true">LOCK</button>
                  </div>
                </div>
                <% } %>
              </form>
              <!--===================================== /MACS form =========================================================== -->
            </div>
            <div class="tab-pane fade pt-2 " id="Tdx" role="tabpanel" aria-labelledby="Tdx-tab">
