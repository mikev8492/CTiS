            <li class="nav-item">
              <a class="nav-link text-secondary active" id="ficoll-tab" data-toggle="tab" href="#ficoll" role="tab"
                aria-controls="ficoll" aria-selected="true">Ficoll</a>
            </li>
            <li class="nav-item">
              <a class="nav-link text-secondary" id="thaw-tab" data-toggle="tab" href="#thaw" role="tab"
                aria-controls="thaw" aria-selected="false">THAW</a>
