            <div class="tab-pane fade pt-2 show active" id="ficoll" role="tabpanel" aria-labelledby="ficoll-tab">
              <!-- ============================================== ficoll form ===========================================================-->
              <form action="/products/carT/<%= data.lot %>/?id=<%= currentLot %>&_method=PUT" method="POST"
                autocomplete="off">
                <div class="form-row">
                  <div class="form-group col-md-6">
                    <label for="fcl_procDate">Process Date</label>
                    <input type="date" class="form-control col-md-6" name="Ficoll[fcl_procDate]"
                      value="<%= data.fcl_procDate %>" required>
                  </div>
                  <div class="form-group col-md-6">
                    <label for="fcl_startTime">Process Time</label>
                    <input type="text" class="timeInput form-control col-md-6" name="Ficoll[fcl_startTime]"
                      value="<%= data.fcl_startTime %>" placeholder="HH:mm (24-hr)"
                      pattern="([0-1]{1}[0-9]{1}|20|21|22|23):[0-5]{1}[0-9]{1}" maxlength="5" required>
                  </div>
                  <div class="form-group col-md-6">
                    <label for="fcl_preTVC">Pre-ficoll TVC</label>
                    <div class="input-group">
                      <input id="tvc" type="number" min="0" step="0.1" class="form-control col-md-4"
                        name="Ficoll[fcl_preTVC]" value="<%= data.fcl_preTVC %>" required>
                      <div class="input-group-append">
                        <span class="input-group-text">x 10<sup>6</sup></span>
                      </div>
                    </div>
                  </div>
                  <div class="form-group col-md-6">
                    <label for="fcl_preVia">Pre-ficoll Viability</label>
                    <div class="input-group">
                      <input id="viability" type="number" min="0" step="0.1" class="form-control col-md-4"
                        name="Ficoll[fcl_preVia]" value="<%= data.fcl_preVia %>" required>
                      <div class="input-group-append">
                        <span class="input-group-text">%</span>
                      </div>
                    </div>
                  </div>
                  <div class="form-group col-md-6">
                    <label for="fcl_postTVC">Post-ficoll TVC</label>
                    <div class="input-group">
                      <input type="number" min="0" step="0.1" class="form-control col-md-4" name="Ficoll[fcl_postTVC]"
                        value="<%= data.fcl_postTVC %>" required>
                      <div class="input-group-append">
                        <span class="input-group-text">x 10<sup>6</sup></span>
                      </div>
                    </div>
                  </div>
                  <div class="form-group col-md-6">
                    <label for="fcl_postVia">Post-ficoll Viability</label>
                    <div class="input-group">
                      <input type="number" min="0" step="0.1" class="form-control col-md-4" name="Ficoll[fcl_postVia]"
                        value="<%= data.fcl_postVia %>" required>
                      <div class="input-group-append">
                        <span class="input-group-text">%</span>
                      </div>
                    </div>
                  </div>
                  <div class="form-group col-md-6">
                    <label for="fcl_equip">Ficoll equipment</label>
                    <select id="inputState" class="form-control col-md-4" name="Ficoll[fcl_equip]" required>
                      <option selected disabled hidden><%= data.fcl_equip %></option>
                      <option>Prodigy</option>
                      <option>Sepax</option>
                    </select>
                  </div>
                  <div class="form-group col-md-6">
                    <label for="fcl_bags">#Bags Cryopreserved</label>
                    <input type="number" min="0" step="1" class="form-control col-md-4" name="Ficoll[fcl_bags]"
                      value="<%= data.fcl_bags %>" required>
                  </div>
                  <div class="form-group col-md-6">
                    <label for="fcl_cryoConc">Cryo Concentration</label>
                    <div class="input-group">
                      <input type="number" min="0" step="0.1" class="form-control col-md-4" name="Ficoll[fcl_cryoConc]"
                        value="<%= data.fcl_cryoConc %>" required>
                      <div class="input-group-append">
                        <span class="input-group-text">x 10<sup>6</sup>/mL</span>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="form-row">
                  <% if(data.fcl_lock === null){ %>
                  <div class="submitBtn col-md-6 col-6">
                    <button type="submit" class="btn btn-warning">Save</button>
                  </div>
                  <div class="lockBtn col-md-6 col-6">
                    <button class="btn btn-danger" name="Ficoll[fcl_lock]" type="submit" value="true">LOCK</button>
                  </div>
                  <% } %>
                </div>

              </form>
              <!--===================================== /ficoll form =========================================================== -->
            </div>
